  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:check": "next lint",
    "format": "next lint --fix && prettier --write \"app/**/*.{ts,tsx,scss}\"",
    "prettier:fix": "prettier --write \"app/**/*.{ts,tsx,scss}\"",
    "prettier:check": "prettier --check \"app/**/*.{ts,tsx,scss}\"",
    "schema:codegen": "gqlg --schemaFilePath ./schema/schema.graphql --destDirPath ./__generated__ --depthLimit 3 && prettier --write --log-level silent __generated__/**/*.gql",
